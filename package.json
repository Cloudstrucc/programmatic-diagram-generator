{
  "name": "azure-diagram-generator",
  "version": "2.1.0",
  "description": "Generate Azure architecture diagrams with AI and publish to GitHub/Azure DevOps",
  "main": "generate-diagram.js",
  "type": "module",
  "scripts": {
    "generate": "node generate-diagram.js",
    "generate:local": "node generate-diagram.js --target local",
    "generate:github": "node generate-diagram.js --target github",
    "generate:devops": "node generate-diagram.js --target devops",
    "generate:all-targets": "node generate-diagram.js --target all",
    "ai:generate": "node ai-diagram.js generate",
    "ai:preview": "node ai-diagram.js preview",
    "ai:publish": "node ai-diagram.js publish",
    "ai:clean": "node ai-diagram.js clean"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "node-fetch": "^3.3.0",
    "pako": "^2.1.0",
    "simple-git": "^3.22.0",
    "commander": "^11.1.0",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}